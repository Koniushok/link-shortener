{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "localStorage": true,
    "window": true,
    "document": true
  },
  "plugins": ["import", "jsx-a11y", "react", "flowtype"]
}
